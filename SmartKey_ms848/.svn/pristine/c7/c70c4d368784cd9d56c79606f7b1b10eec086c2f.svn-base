<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.odm.smartkey"
    android:versionCode="20180619"
    android:versionName="20180619" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="25" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.tcl.ShortcutKey" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:name="com.odm.smartkey.receiver.App"
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen" >
        <activity
            android:name="com.odm.smartkey.SmartKeyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <!-- <category android:name="android.intent.category.DEFAULT"/> -->
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.odm.smartkey.SingleListenerRemindActivity"
            android:exported="true" />

        <receiver
            android:name="com.odm.smartkey.receiver.MyReceiver"
            android:exported="true" >
            <intent-filter>
                <action android:name="action_child_mode" />
                <action android:name="com.tclodm.intent.action.singleListenOff" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.moka.sleepmode" />
            </intent-filter>
            <intent-filter>
                <action android:name="action_health_eye" />
            </intent-filter>
            <intent-filter android:priority="500" >
                <action android:name="com.tcl.ShortcutKey" />
                <action android:name="com.tcl.tv.diandu" />
                <action android:name="android.intent.action.ENTER_HOME" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>
    </application>

</manifest>